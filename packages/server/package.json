{
  "name": "@anthropic/fingerprint-server",
  "version": "1.0.0",
  "description": "Fingerprint API server with storage and matching",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "prisma generate && tsc",
    "dev": "tsx watch src/index.ts",
    "start": "prisma db push && node dist/index.js",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "prisma": "^5.7.0",
    "tsx": "^4.6.2"
  },
  "license": "MIT"
}
