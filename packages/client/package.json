{
  "name": "@anthropic/fingerprint-client",
  "version": "1.0.0",
  "description": "Browser fingerprinting library",
  "main": "dist/fingerprint.umd.js",
  "module": "dist/fingerprint.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/fingerprint.esm.js",
      "require": "./dist/fingerprint.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "clean": "rm -rf dist",
    "test": "jest",
    "lint": "eslint src --ext .ts"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.5",
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.6.0",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "jest": "^29.7.0",
    "@types/jest": "^29.5.10",
    "ts-jest": "^29.1.1"
  },
  "keywords": [
    "fingerprint",
    "browser",
    "analytics"
  ],
  "license": "MIT"
}
